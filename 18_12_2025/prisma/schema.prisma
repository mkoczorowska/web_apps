generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Car {
  id           Int      @id @default(autoincrement())
  manufacturer String
  model        String
  color        String
  fuel         String
  vin          String   @unique
  createdAt    DateTime @default(now())
  updatedAt    DateTime
}
